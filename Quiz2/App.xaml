<Application x:Class="Quiz2.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:viewmodels="clr-namespace:Quiz2.ViewModels"
             xmlns:views="clr-namespace:Quiz2.Views">
    <Application.Resources>
        <DataTemplate DataType="{x:Type viewmodels:ChangeCategoryViewModel}">
            <views:ChangeCategoryView />
        </DataTemplate>
        <DataTemplate DataType="{x:Type viewmodels:UserControl2ViewModel}">
            <views:UserControl2 />
        </DataTemplate>
    </Application.Resources>
</Application>